{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "id": "app-hosting-backend",
      "region": "us-central1"
    },
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "backend",
          "region": "us-central1"
        }
      }
    ]
  },
  "apphosting": [
    {
      "backendId": "app-hosting-backend"
    }
  ]
}
